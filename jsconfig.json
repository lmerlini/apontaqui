{
    "include": [
        "src/**/*.js",
        "src/**/*.vue",
    ],
    "exclude": [
        "node_modules",
        "**/dist/*"
    ],
    "compilerOptions": {
        "target": "esnext",
        "skipLibCheck": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}